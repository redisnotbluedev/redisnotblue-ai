# OpenAI Proxy Server - Example Environment Variables
# Copy this file to .env and fill in your actual API keys

# Primary OpenAI Provider - Multiple API Keys for Round-Robin
OPENAI_API_KEY_1=sk-xxxxxxxxxxxxxxxxxxxxx
OPENAI_API_KEY_2=sk-xxxxxxxxxxxxxxxxxxxxx
OPENAI_API_KEY_3=sk-xxxxxxxxxxxxxxxxxxxxx

# Backup OpenAI Provider - Multiple API Keys
OPENAI_BACKUP_API_KEY_1=sk-xxxxxxxxxxxxxxxxxxxxx
OPENAI_BACKUP_API_KEY_2=sk-xxxxxxxxxxxxxxxxxxxxx

# Single API Key (if using backward-compatible config)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxx
OPENAI_BACKUP_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxx

# Production API Keys (comma-separated for environment variable config)
OPENAI_PROD_KEYS=sk-key1,sk-key2,sk-key3

# Server Configuration
CONFIG_PATH=config/config.yaml
HOST=0.0.0.0
PORT=8000

# Logging
LOG_LEVEL=INFO

# API Request Timeout (seconds)
API_TIMEOUT=60

# Cooldown Period for Failed API Keys (seconds)
COOLDOWN_SECONDS=600

# Maximum Retries per Provider
MAX_RETRIES=3